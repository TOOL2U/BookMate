{
  "name": "bookmate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node test-all.js",
    "test:quick": "node test-all.js --skip-build",
    "test:verbose": "node test-all.js --verbose",
    "dev:persistent": "chmod +x scripts/persistent-server.sh && ./scripts/persistent-server.sh",
    "dev:status": "tail -f server.log",
    "dev:stop": "kill $(cat .server.pid 2>/dev/null) 2>/dev/null || echo 'No server running'",
    "dev:check": "lsof -i :3000 || echo 'Port 3000 is free'",
    "inspect-sheet": "node scripts/inspect-sheet-validation.js",
    "fetch-dropdowns": "node scripts/fetch-dropdown-values.js",
    "sync-dropdowns": "node scripts/fetch-live-dropdowns.js",
    "sync": "node sync-sheets.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^11.2.10",
    "googleapis": "^164.1.0",
    "lucide-react": "^0.548.0",
    "next": "^15.0.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.3.0",
    "tailwindcss": "^4.1.16",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.39",
    "typescript": "^5.5.3"
  }
}
